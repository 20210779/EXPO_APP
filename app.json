{
  "expo": {
    "name": "Expo_Mobile",
    "slug": "Expo_Mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon_app.jpeg",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon_app.jpeg",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "package": "com.jupadi140949.Expo_Mobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "nativeModuleName": "expoLocation"
        }
      ],
      "expo-localization"
    ],
    "extra": {
      "eas": {
        "projectId": "03c4077d-4882-4020-975f-c066bfcb6705"
      }
    }
  }
}
