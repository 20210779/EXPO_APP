{
  "expo": {
    "name": "Expo_Mobile",
    "slug": "Expo_Mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon_app.jpeg",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon_app.jpeg",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "nativeModuleName": "expoLocation"
        }
      ],
      "expo-localization"
    ]
  }
}
